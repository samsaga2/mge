prog = (<nl>|<ws>)? sub (<nl> sub)* (<nl>|<ws>)?

nl = #'[\n\r]+'
ws = #'\s+'
<id> = #'[a-zA-Z_][a-zA-Z_0-9]*'
num = #'-?[0-9]+'
str = <'"'> #'[^"]*' <'"'>

sub = <'sub'> <ws> id <nl> op* <'endsub'>
<op> = <ws>? sprite-ops <nl>
<argcoord> = <'('> <ws>? num <ws>? <','> <ws>? num <ws>? <')'>

<sprite-ops> = (sprite-pos|sprite-image|sprite-move|sprite-color)
sprite-pos = <'pos'> <ws> argcoord
sprite-image = <'image'> <ws> str
sprite-move = <'move'> <ws> argcoord
sprite-color = <'color'> <ws> num
